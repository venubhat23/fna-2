        "exec": [
          "// Auto-extract tokens from login responses",
          "if (pm.response.code === 200) {",
          "    const responseJson = pm.response.json();",
          "    if (responseJson.success && responseJson.data && responseJson.data.token) {",
          "        const role = responseJson.data.role;",
          "        const token = responseJson.data.token;",
          "        ",
          "        if (role === 'agent') {",
          "            pm.environment.set('agent_token', token);",
          "            pm.environment.set('auth_token', token);",
          "        } else if (role === 'customer') {",
          "            pm.environment.set('customer_token', token);",
          "            pm.environment.set('auth_token', token);",
          "        } else if (role === 'sub_agent') {",
          "            pm.environment.set('sub_agent_token', token);",
          "            pm.environment.set('auth_token', token);",
          "        }",
          "        ",
          "        console.log('Token set for role:', role);",
          "    }",
          "}"
        ]
      }
